<?xml version="1.0" encoding="ASCII"?>
<behavior:ListsOfBehaviors
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:behavior="http://www.eventDrivenScenario.org/behavior"
    xmlns:ca.mcgill.ecse.scenario="ca.mcgill.ecse.scenario">
  <behaviorLists
      actor="//@scenario/@dynamicComponents.0"/>
  <behaviorLists
      actor="//@scenario/@dynamicComponents.1"/>
  <allBehaviors
      name="LaneChangeBehavior">
    <parameters
        type="LaneSegment"
        name="laneSectionToSwitch"/>
    <parameters
        type="boolean"
        value="False"
        name="is_oppositeTraffic"/>
    <parameters
        type="int"
        value="5"
        name="target_speed"/>
  </allBehaviors>
  <allBehaviors
      name="TurnBehavior">
    <parameters
        type="Trajectory"
        name="trajectory"/>
  </allBehaviors>
  <allBehaviors
      name="FollowLaneBehavior">
    <parameters
        type="Path"
        name="laneToFollow"/>
  </allBehaviors>
  <scenario
      firstScene="//@scenario/@scenes.0">
    <scenery>
      <roads
          name="r1">
        <lanes
            name="l0"
            rightLane="//@scenario/@scenery/@roads.0/@lanes.1"
            oppDirLaneInSec="//@scenario/@scenery/@roads.0/@lanes.1"/>
        <lanes
            name="l1"
            laneId="1"
            leftLane="//@scenario/@scenery/@roads.0/@lanes.0"
            oppDirLaneInSec="//@scenario/@scenery/@roads.0/@lanes.0"/>
        <contains
            xsi:type="ca.mcgill.ecse.scenario:Crosswalk"
            name="c1"/>
      </roads>
    </scenery>
    <dynamicComponents
        xsi:type="ca.mcgill.ecse.scenario:Pedestrian"
        name="p1"
        instances="//@scenario/@scenes.0/@abstractDynamicInstances.0"/>
    <dynamicComponents
        xsi:type="ca.mcgill.ecse.scenario:Car"
        name="c2"
        instances="//@scenario/@scenes.0/@abstractDynamicInstances.1"/>
    <scenes
        name="initialScene">
      <abstractDynamicInstances
          name="s0-p1"
          speed="AroundLimit"
          actor="//@scenario/@dynamicComponents.0">
        <relations
            xsi:type="ca.mcgill.ecse.scenario:IsPlacedOn"
            target="//@scenario/@scenery/@roads.0/@contains.0"/>
      </abstractDynamicInstances>
      <abstractDynamicInstances
          name="s0-c2"
          speed="AroundLimit"
          actor="//@scenario/@dynamicComponents.1">
        <relations
            xsi:type="ca.mcgill.ecse.scenario:IsPlacedOn"
            target="//@scenario/@scenery/@roads.0/@lanes.1"/>
        <relations
            xsi:type="ca.mcgill.ecse.scenario:HasInFront"
            target="//@scenario/@scenery/@roads.0/@contains.0"/>
        <relations
            xsi:type="ca.mcgill.ecse.scenario:IsFarFrom"
            target="//@scenario/@dynamicComponents.0"/>
        <relations
            xsi:type="ca.mcgill.ecse.scenario:HasToItsLeft"
            target="//@scenario/@dynamicComponents.0"/>
      </abstractDynamicInstances>
    </scenes>
  </scenario>
</behavior:ListsOfBehaviors>
